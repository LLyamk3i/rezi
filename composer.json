{
    "name": "afriklabthech/resi-api",
    "type": "project",
    "description": "RESI api app.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "archtechx/enums": "^0.3.2",
        "filament/filament": "^2.17.50",
        "guysolamour/laravel-cinetpay": "^2.0",
        "guzzlehttp/guzzle": "^7.7.0",
        "laravel/framework": "^10.15.0",
        "laravel/sanctum": "^3.2.5",
        "laravel/tinker": "^2.8.1"
    },
    "require-dev": {
        "doctrine/dbal": "^3.6.4",
        "driftingly/rector-laravel": "^0.17.0",
        "fakerphp/faker": "^1.23.0",
        "knuckleswtf/scribe": "^4.22.0",
        "laravel/pint": "^1.10.5",
        "laravel/telescope": "^4.15.2",
        "mockery/mockery": "^1.6.2",
        "nunomaduro/collision": "^7.7.0",
        "nunomaduro/larastan": "^2.6.3",
        "pestphp/pest": "^2.8.3",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1.3",
        "phpstan/phpstan-strict-rules": "^1.5.1",
        "qossmic/deptrac-shim": "^1.0.2",
        "rector/rector": "^0.15.25",
        "spatie/laravel-ignition": "^2.2.0",
        "spatie/laravel-login-link": "^1.1.3"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
