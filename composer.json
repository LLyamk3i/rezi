{
    "name": "afriklabthech/resi-api",
    "type": "project",
    "description": "RESI api app.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "archtechx/enums": "^0.3.2",
        "cheesegrits/filament-google-maps": "^3.0.11",
        "dedoc/scramble": "^0.8.5",
        "doctrine/dbal": "^3.7.2",
        "filament/filament": "^3.1.9",
        "guzzlehttp/guzzle": "^7.8.1",
        "laravel/framework": "^10.34.2",
        "laravel/sanctum": "^3.3.2",
        "laravel/tinker": "^2.8.2",
        "league/glide-laravel": "^1.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^0.17.0",
        "fakerphp/faker": "^1.23.0",
        "filament/upgrade": "^3.0-stable",
        "laradumps/laradumps": "^2.2.1",
        "laravel/pint": "^1.13.6",
        "laravel/telescope": "^4.17.2",
        "mockery/mockery": "^1.6.6",
        "nunomaduro/collision": "^7.10.0",
        "nunomaduro/larastan": "^2.6.4",
        "opcodesio/log-viewer": "^2.5.6",
        "pestphp/pest": "^2.26.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-strict-rules": "^1.5.2",
        "qossmic/deptrac-shim": "^1.0.2",
        "rector/rector": "^0.15.25",
        "spatie/laravel-ignition": "^2.3.1",
        "spatie/laravel-login-link": "^1.1.3"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
